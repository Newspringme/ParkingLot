<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cnzk.mapper.VipMapper">
    <!--根据id查询vip类型名-->
    <select id="queryVipInfo" resultType="com.cnzk.pojo.TbVip">
      select * from tb_vip where vip_id = #{vipId}
    </select>
</mapper>